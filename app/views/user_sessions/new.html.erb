<h1>Acceso</h1><br />
<%=link_to  image_tag("unet-blanco.png", :class=> "login"), root_path%>
<% form_for @user_session do |f| %>
  <%= f.error_messages %>
  <p><%= f.label :login %><br />
  <%= f.text_field :login %><br />
  </p>
  <p>
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  </p>
  <p>
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  </p>
  <p>
  <%= f.submit "Login" %>	
  </p>
<% end %>