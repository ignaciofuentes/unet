<h1>New comentario</h1>

<% form_for(@comentario) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :autor %><br />
    <%= f.text_field :autor %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :texto %><br />
    <%= f.text_area :texto %>
  </p>
  <p>
    <%= f.label :comentable_id %><br />
    <%= f.text_field :comentable_id %>
  </p>
  <p>
    <%= f.label :comentable_type %><br />
    <%= f.text_field :comentable_type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', comentarios_path %>